<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="ED_RemittanceSearch">
    <page>/ED_RemittanceSearch.jspx</page>
    <input-page-parameter>
      <from-value>#{param.ParamOrgId}</from-value>
      <to-value>#{viewScope.OrgId}</to-value>
    </input-page-parameter>
    <input-page-parameter>
      <from-value>#{param.ParamUserId}</from-value>
      <to-value>#{viewScope.UserID}</to-value>
    </input-page-parameter>
    <input-page-parameter>
      <from-value>#{param.ParamRespId}</from-value>
      <to-value>#{viewScope.RespId}</to-value>
    </input-page-parameter>
    <input-page-parameter>
      <from-value>#{param.ParamRespApplId}</from-value>
      <to-value>#{viewScope.RespApplId}</to-value>
    </input-page-parameter>
  </view>
  <view id="EDRemittance">
    <page>/EDRemittance.jspx</page>
    <input-page-parameter>
      <from-value>#{param.ParamOrgId}</from-value>
      <to-value>#{viewScope.OrgId}</to-value>
    </input-page-parameter>
    <input-page-parameter>
      <from-value>#{param.ParamUserId}</from-value>
      <to-value>#{viewScope.UserId}</to-value>
    </input-page-parameter>
    <input-page-parameter>
      <from-value>#{param.ParamRespId}</from-value>
      <to-value>#{viewScope.RespId}</to-value>
    </input-page-parameter>
    <input-page-parameter>
      <from-value>#{param.ParamRespApplId}</from-value>
      <to-value>#{viewScope.RespApplId}</to-value>
    </input-page-parameter>
  </view>
  <control-flow-rule id="__1">
    <from-activity-id id="__2">ED_RemittanceSearch</from-activity-id>
    <control-flow-case id="__4">
      <from-outcome id="__5">Create/Edit Remittance</from-outcome>
      <to-activity-id id="__3">EDRemittance</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule id="__6">
    <from-activity-id id="__7">EDRemittance</from-activity-id>
    <control-flow-case id="__8">
      <from-outcome id="__10">Search Remittance</from-outcome>
      <to-activity-id id="__9">ED_RemittanceSearch</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
</adfc-config>

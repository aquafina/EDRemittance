  <SQLQuery>
    <![CDATA[SELECT   hdr.comm_inv_date, hdr.comm_inv_num, hdr.customer_id,
         /*NVL (  SUM (line.quantity * line.unit_selling_price)
              + SUM (hdr.freight_charges)
              + SUM (hdr.insurance_charges)
              + SUM (hdr.other_charges)
              + SUM (hdr.discount),
              0
             )*/ to_number(pwc_ed_comm_inv_amount(hdr.comm_inv_num,hou.organization_id)) due_amount,
             sum(hdr.discount) discount
    FROM pwc_ed_commercial_invoice_hdr hdr,
         hr_organization_units hou,
         pwc_ed_commercial_invoice_l line
   WHERE hdr.org_id = hou.organization_id
     AND hdr.header_id = line.header_id
     AND hou.organization_id = NVL (:p_org_id, hou.organization_id)
     AND hdr.customer_id = NVL (:p_cust_id, hdr.customer_id)
GROUP BY hdr.comm_inv_date, hdr.comm_inv_num, hdr.customer_id, hou.organization_id]]>
  </SQLQuery>
